#include <iostream>
 
int main()
{
    std::cout << "Hola! Trataré de adivinar un número.";
    std::cout << "Piensa un número entre 1 y 10.";
    sleep(5)	
    std::cout << "Ahora multiplicalo por 9.";
    sleep(5)
    std::cout << "Si el número tiene 2 dígitos, súmalos entre si: Ej. 36->3+6=9. Si tu número tiene un solo dígito, súmale 0.";
    sleep(5)
    std::cout << "Al número resultante súmale 4.";
    sleep(10)	
    std::cout << "Muy bien. El resultrado es 13 :)"; 
}

